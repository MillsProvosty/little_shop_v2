<% @items.each do |item| %>
  <div id="item-<%=item.id%>">
    <%= link_to item.name, item_path(item) %>
    <a href="<%=item_path(item)%>">
      <img src="<%= item.image %>"/>
    </a>
    <p><%= item.user.name%></p>
    <p>In Stock: <%=item.inventory %></p>
    <p>Price: <%=item.price.to_f%></p>
  </div>
<% end %>

