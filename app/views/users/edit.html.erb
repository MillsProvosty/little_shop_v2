<%= form_for current_user, url: user_edit_path, method: :patch do |f| %>

<%= f.label :name  %>
<%= f.text_field :name %>


<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :address  %>
<%= f.text_field :address%>

<%= f.label :city  %>
<%= f.text_field :city%>

<%= f.label :state  %>
<%= f.text_field :state%>

<%= f.label :zip  %>
<%= f.text_field :zip%>

<%= f.label :password  %>
<%= f.text_field :password, value: "" %>

<%= f.submit "Submit Edits" %>

<% end %>
