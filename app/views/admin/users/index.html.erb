<h1>Users In the System</h1>
<div id="all-users">
<% @users.each do |user| %>
<div id="user-<%= user.id %>" class="users">
  <p><%= link_to user.name, admin_user_path(user) %></p>
  <p><%= user.created_at %></p>
  <%= button_to "Enable Merchant", admin_update_merchant_path(user.id), method: :update %>
</div>
<% end %>
</div>
